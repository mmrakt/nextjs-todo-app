{
  "name": "nextjs-todo-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "export": "next build && next export -o dist",
    "l": "eslint --ext .ts,.tsx ./src",
    "l:f": "eslint --fix --ext .ts,.tsx ./src",
    "f": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json}'",
    "f:w": "onchange '**/*.{ts,tsx,js}' -e '.next/**/*.js' -- prettier --config ./.prettierrc --write {{changed}}",
    "test": "jest --watch",
    "test:c": "jest --collect-coverage",
    "migrate": "prisma migrate dev --schema prisma/schema.prisma",
    "generate": "prisma generate --schema prisma/schema.prisma"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.1.1",
    "@date-io/date-fns": "^2.10.6",
    "@hookform/error-message": "^0.0.5",
    "@next-auth/prisma-adapter": "^1.0.1",
    "@next/bundle-analyzer": "^12.0.7",
    "@prisma/client": "3.7.0",
    "@tailwindcss/forms": "^0.2.1",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.2",
    "@testing-library/react-hooks": "^4.0.1",
    "@testing-library/user-event": "^12.6.0",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/react-test-renderer": "^17.0.0",
    "autoprefixer": "^10.2.1",
    "daisyui": "^2.14.3",
    "date-fns": "^2.16.1",
    "dayjs": "^1.10.7",
    "dotenv": "^8.2.0",
    "framer-motion": "^6.2.6",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "jest-dom": "^4.0.0",
    "jwt-decode": "^3.1.2",
    "lint-staged": "^10.5.3",
    "minireset.css": "^0.0.6",
    "next": "^12.0.7",
    "next-auth": "^4.0.6",
    "nextjs-progressbar": "^0.0.13",
    "onchange": "^7.1.0",
    "postcss": "8.1.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^7.25.0",
    "react-icons": "^4.3.1",
    "react-image-crop": "^8.6.6",
    "react-modal": "^3.15.1",
    "react-query": "^3.34.7",
    "react-responsive": "^8.2.0",
    "react-router-dom": "^5.2.0",
    "react-test-renderer": "^17.0.1",
    "tailwindcss": "^3.0.8",
    "ts-jest": "^26.4.4",
    "typescript": "^4.5.0"
  },
  "devDependencies": {
    "@chakra-ui/react": "^1.7.3",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@types/autosize": "^3.0.7",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^7.23.0",
    "eslint-config-next": "^12.0.7",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-webpack-plugin": "^3.1.1",
    "prettier": "^2.2.1",
    "prisma": "^3.7.0",
    "ts-node": "^9.1.1"
  },
  "moduleDirectories": [
    "node_modules",
    "src"
  ]
}
